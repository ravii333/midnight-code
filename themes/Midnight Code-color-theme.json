    "progressBar.background": "#2289FE",

    "list.activeSelectionBackground": "#2289FE30",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#2289FE10",
    "list.hoverBackground": "#2289FE15",
    "list.hoverForeground": "#FFFFFF",
    "list.focusBackground": "#2289FE30",
    "list.focusForeground": "#FFFFFF",
    "list.highlightForeground": "#2289FE",
    "list.errorForeground": "#FF6E6E",
    "list.warningForeground": "#FFB870",

    "activityBar.background": "#1E293B",
    "activityBar.foreground": "#B9D8FF",
    "activityBar.inactiveForeground": "#4B5563",
    "activityBar.border": "#1E293B",
